{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": [
            true,
            80
        ],
        "quotemark": [
            "single"
        ],
        "no-console": [
            false
        ],
        "only-arrow-functions": [
            false
        ],
        "no-var-requires": [
            false
        ]
    }
}